    "version": "0.2.0",
    "configurations": [
        {
            "name": "Compile All",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/compile.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Compile Photos",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/compile.py",
            "console": "integratedTerminal",
            "args": [
                "photos",
                "-v",
                "2"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
    ]
}